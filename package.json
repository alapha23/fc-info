{
  "name": "fc-info",
  "version": "0.0.12",
  "description": "fc-info component",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run doc && tsc -w",
    "build": "rm -rf node_modules && rm -rf package-lock.json && npm i && ncc build src/index.ts -m -e @serverless-devs/core -o lib",
    "test": "jest --testNamePattern '^(?!Integration::)",
    "test:cov": "jest --coverage",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.0.*",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "@alicloud/fc2": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "jest": "^26.4.0",
    "sinon": "^5.0.7",
    "ts-jest": "^26.2.0",
    "@types/jest": "^26.0.10",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
